#include <iostream>
using namespace std;
int main()
{

float celsius, fahren;

cout<<"Geben Sie die Temperatur in Fahrenheit an: ";
cin>>fahren;
celsius= ((fahren-32)*5/9);
cout<<celsius;
  getchar();
  return 0;
}
